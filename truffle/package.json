{
  "name": "truffle",
  "version": "1.0.0",
  "description": "HelloDefi project",
  "main": "truffle-config.js",
  "dependencies": {},
  "scripts": {
    "compile": "npm run clean-abi && npm run clean-mocks && truffle compile --reset && truffle compile --contracts_directory test/helpers --reset && npm run generate-types && npm run clean-mocks",
    "generate-types": "typechain --target=truffle-v5 --out-dir types '../client/src/contracts/*.json'",
    "clean-mocks": "rimraf ../client/src/contracts/Mock*.json ../client/src/contracts/Context.json",
    "clean-types": "rimraf types/*",
    "clean-abi": "rimraf ../client/src/contracts/*.json",
    "test": "truffle test"
  },
  "author": "Guillaume Debavelaere",
  "license": "MIT",
  "devDependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/contracts-upgradeable": "^4.8.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@truffle/hdwallet-provider": "^2.1.2",
    "@typechain/truffle-v5": "^8.0.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.10",
    "chai": "^4.3.7",
    "chai-bn": "^0.3.1",
    "dotenv": "^16.0.3",
    "eth-gas-reporter": "^0.2.25",
    "rimraf": "^3.0.2",
    "truffle": "^5.6.8",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  }
}
